{
  "name": "api-docs",
  "version": "1.0.0",
  "description": "Swagger documentation",
  "scripts": {
    "start": "gulp serve",
    "lint": "gulp lint",
    "precommit": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfrastructuredev/api-docs.git"
  },
  "authors": [
    "Chuck \"MANCHUCK\" Reeves",
    "Michael Pepe"
  ],
  "license": "UNLICENSED",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 50,
    "temp-directory": "/tmp/.nyc_output",
    "exclude": [
      "node_modules",
      "tests"
    ],
    "include": [
      "src/**/*"
    ],
    "all": true,
    "extension": [
      ".js",
      ".jsx"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "spread": true,
        "experimentalObjectRestSpread": true
      }
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "curly": "error",
      "require-jsdoc": [
        "error"
      ],
      "valid-jsdoc": [
        "error"
      ],
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1,
          "MemberExpression": 1
        }
      ]
    }
  },
  "dependencies": {
    "@nterprise/common-js": "^3.0.5",
    "aws-serverless-express": "^3.3.6",
    "chance": "^1.1.3",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "json-schema-faker": "^0.5.0-rc23",
    "jsonpath": "^1.0.2",
    "lodash": "^4",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "request-promise-native": "^1.0.8",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.1.2",
    "swagger2": "^1.0.5",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "git-pre-commit": "^2.1.4",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-fn": "0.0.3",
    "gulp-nodemon": "^2.4.2",
    "gulp-shell": "^0.7.1",
    "json-schema-ref-parser": "^7.1.2",
    "swagger-cli": "^2.3.4"
  }
}
