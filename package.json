{
  "name": "api-docs",
  "version": "1.0.0",
  "description": "Swagger documentation",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfrastructuredev/api-docs.git"
  },
  "scripts": {
    "precommit": "echo 'This is not the commit hook we are looking for'"
  },
  "authors": [
    "Chuck \"MANCHUCK\" Reeves",
    "Michael Pepe"
  ],
  "license": "UNLICENSED",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 50,
    "temp-directory": "/tmp/.nyc_output",
    "exclude": [
      "node_modules",
      "tests"
    ],
    "include": [
      "src/**/*"
    ],
    "all": true,
    "extension": [
      ".js",
      ".jsx"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "curly": "error",
      "require-jsdoc": [
        "error"
      ],
      "valid-jsdoc": [
        "error"
      ],
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1,
          "MemberExpression": 1
        }
      ],
      "no-console": "error",
      "operator-linebreak": [
        "error",
        "before"
      ]
    }
  },
  "dependencies": {
    "ajv": "^6.12.4",
    "chance": "^1.1.7",
    "faker": "^5.1.0",
    "json-schema-faker": "^0.5.0-rcv.29",
    "jsonpath": "^1.0.2",
    "lodash": "^4",
    "winston": "^3.3.3",
    "yaml": "^1.10.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@nterprise/niagara-winston-logger": "^1.0.3",
    "eslint": "^7.8.1",
    "eslint-config-google": "^0.14.0",
    "glob": "^7.1.6",
    "json-schema-ref-parser": "^9.0.6",
    "widdershins": "^4.0.1"
  }
}
