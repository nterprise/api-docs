{
  "name": "api-docs",
  "version": "1.0.0",
  "description": "Swagger documentation",
  "scripts": {
    "start": "gulp serve",
    "lint": "gulp lint",
    "precommit": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfrastructuredev/api-docs.git"
  },
  "authors": [
    "Chuck \"MANCHUCK\" Reeves",
    "Michael Pepe"
  ],
  "license": "UNLICENSED",
  "eslintConfig": {
    "env": {
      "browser": false,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "settings": {
      "propWrapperFunctions": [
        "forbidExtraProps",
        {
          "property": "freeze",
          "object": "Object"
        },
        {
          "property": "myFavoriteWrapper"
        }
      ],
      "linkComponents": [
        "Hyperlink",
        {
          "name": "Link",
          "linkAttribute": "to"
        }
      ]
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "curly": "error",
      "require-jsdoc": [
        "error"
      ],
      "valid-jsdoc": [
        "error"
      ],
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1,
          "MemberExpression": 1
        }
      ]
    }
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "swagger": "^0.7.5",
    "express": "^4.16.4",
    "swagger-ui-express": "^4.0.2",
    "swagger2": "^1.0.4"
  },
  "devDependencies": {
    "git-pre-commit": "^2.1.4",
    "glob": "^7.1.3",
    "gulp": "^4.0.1",
    "gulp-cli": "^2.2.0",
    "gulp-fn": "0.0.3",
    "gulp-nodemon": "^2.4.2",
    "gulp-shell": "^0.7.0",
    "lodash": "^4.17.11",
    "swagger-cli": "^2.2.1"
  }
}
