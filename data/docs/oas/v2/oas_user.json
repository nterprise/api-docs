  "paths": {
    "/users": {
      "$ref": "./paths/user/users.json"
    },
    "/users/{user_id}": {
      "$ref": "./paths/user/user.json"
    }
  },
  "components": {
    "schemas": {
      "User": {
        "$ref": "components/schemas/user.json"
      }
    }
