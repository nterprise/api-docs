{
  "openapi": "3.0.3",
  "x-api": "niagara",
  "x-nter-docs-parent": "Niagara API",
  "x-nter-docs-nav-order": 18,
  "x-nter-docs-redirect-from": [
    "/rel/users",
    "/rel/user",
    "/rel/note-created-by"
  ],
  "info": {
    "version": "2.0.0",
    "description": "API for the nterprise application",
    "title": "Users"
  },
  "servers": [
    {
