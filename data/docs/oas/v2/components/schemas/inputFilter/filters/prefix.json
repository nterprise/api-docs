{
  "description": "Add a prefix to the start of a string. If the string already start with the prefix, it will not prepend.",
  "type": "object",
  "required": [
    "type",
    "options"
  ],
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "prefix"
      ]
    },
    "options": {
      "type": "object",
      "required": [
        "prefix"
      ],
      "properties": {
        "prefix": {
          "type": "string",
          "description": "The prefix to add",
          "example": "MAN-",
          "x-examples": [
            "MAN-"
          ]
        }
      }
    }
  }
}
