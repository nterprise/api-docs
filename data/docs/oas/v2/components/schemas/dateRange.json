{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://docs.nterprise.com/schemas/niagara/dateRange.json",
  "type": "object",
  "description": "Active date range for an entity",
  "properties": {
    "start_date": {
      "type": "string",
      "format": "date-time",
      "description": "Start date",
      "example": "2019-08-07T18:10:53+00:00",
      "x-examples": [
        "2019-08-07T18:10:53+00:00"
      ]
    },
    "end_date": {
      "type": "string",
      "format": "date-time",
      "description": "End date",
      "example": "2020-08-07T18:10:53+00:00",
      "x-examples": [
        "2020-08-07T18:10:53+00:00"
      ]
    }
  }
}
