{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://docs.nterprise.com/schemas/niagara/batch.json",
  "type": "object",
  "description": "Defines the properties for a work order batch",
  "additionalProperties": false,
  "required": [
    "queue"
  ],
  "x-niagara-model": "Batch",
  "properties": {
    "batch_id": {
      "type": "string",
      "description": "The identifier for the batch",
      "readOnly": true,
      "example": "59c834ad-d9cc-4e0f-a569-8f4e52c351b9",
      "x-examples": [
        "59c834ad-d9cc-4e0f-a569-8f4e52c351b9"
      ],
      "pattern": "^[0-9a-zA-Z-_]+$"
    },
    "queue": {
      "x-niagara-ref": "./queue.json#/properties/queue_id",
      "$ref": "./queue.json"
    },
    "order": {
      "type": "integer",
      "description": "Order for the queue",
      "example": 1,
      "x-examples": [
        1
      ],
      "format": "int32"
    },
    "number_cycles": {
      "type": "integer",
      "description": "The number of cycles for this batch",
      "format": "int32",
      "example": 20,
      "x-examples": [
        20
      ]
    },
    "cycles": {
      "$ref": "./cycle.json"
    },
    "work_order": {
      "x-niagara-ref": "./workOrder.json#/properties/work_order_id",
      "$ref": "./workOrder.json"
    }
  },
  "example": {
    "$ref": "../examples/batch.json"
  },
  "allOf": [
    {
      "x-niagara-raw-ref": "#/properties/entityCommon",
      "$ref": "./entityCommon.json"
    }
  ]
}
