{
  "stop_at": 288,
  "time": 713,
  "type": "wait",
  "label": "Wait for cable",
  "goto": "complete",
  "listen_for": [
    {
      "event": "WOR.created",
      "event_value": {
        "variable": "$.parts.available",
        "greater_than_equals": "42"
      }
    },
    {
      "event": "PART.deleted",
      "event_value": {
        "variable": "$.parts.available",
        "greater_than_equals": "42"
      }
    },
    {
      "event": "USER.detached",
      "event_value": {
        "variable": "$.parts.available",
        "greater_than_equals": "42"
      }
    },
    {
      "event": "CUS.removed",
      "event_value": {
        "variable": "$.parts.available",
        "greater_than_equals": "42"
      }
    }
  ],
  "goto-fail": "process-fail",
  "context": [
    {
      "key": "$.unit.status",
      "value": "$.buzz",
      "lock": false,
      "ignore": false
    },
    {
      "key": "extract_fields",
      "value": "Pending",
      "lock": false,
      "ignore": false
    },
    {
      "key": "extract_fields",
      "value": "Pending",
      "lock": true,
      "ignore": true
    }
  ],
  "onError": {
    "finally": {
      "goto": "failed",
      "actions": [
        {
          "type": "email",
          "options": {
            "to": "bob@example.com",
            "from": "alice@example.com"
          }
        }
      ]
    },
    "retry": 3
  },
  "onComplete": {
    "actions": [
      {
        "type": "email",
        "options": {
          "to": "bob@example.com",
          "from": "alice@example.com"
        }
      },
      {
        "type": "email",
        "options": {
          "to": "bob@example.com",
          "from": "alice@example.com"
        }
      },
      {
        "type": "email",
        "options": {
          "to": "bob@example.com",
          "from": "alice@example.com"
        }
      }
    ]
  }
}