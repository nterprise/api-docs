{
  "openapi": "3.0.3",
  "x-api": "niagara",
  "x-nter-docs-parent": "Niagara API",
  "x-nter-docs-nav-order": 14,
  "x-nter-docs-redirect-from": [
    "/rel/queues",
    "/rel/queue",
    "/rel/batch-queue"
  ],
  "info": {
    "version": "2.0.0",
    "description": "API for the nterprise application",
    "title": "Queues"
  },
  "servers": [
    {
      "url": "https://{environment}.nterprise.com",
      "variables": {
        "environment": {
          "default": "api",
          "enum": [
            "api",
            "api.dev"
          ]
        }
      }
    }
  ],
  "paths": {
    "/queues": {
      "$ref": "./paths/queue/queues.json"
    },
    "/queues/{queue_id}": {
      "$ref": "./paths/queue/queue.json"
    },
    "/queues/{queue_id}/batches": {
      "$ref": "./paths/queue/batches.json"
    }
  },
  "components": {
    "schemas": {
      "Queue": {
        "$ref": "components/schemas/queue.json"
      }
    }
  }
}
