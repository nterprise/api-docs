{
  "operationId": "deleteCustomerContact",
  "summary": "Deletes a contact for a customer",
  "description": "This will remove the contact from the customer",
  "tags": [
    "Customer"
  ],
  "parameters": [
    {"$ref": "../../../components/parameters/customerId.json"},
    {"$ref": "../../../components/parameters/contactId.json"}
  ],
  "responses": {
    "205": {
      "description": "Contact deleted"
    },
    "401": {
      "description": "Authorization invalid",
      "$ref": "../../../components/responses/unauthorized.json"
    },
    "403": {
      "description": "User is not allowed to delete this contact",
      "$ref": "../../../components/responses/forbidden.json"
    },
    "404": {
      "description": "Contact or Customer not found",
      "$ref": "../../../components/responses/notFound.json"
    }
  }
}
