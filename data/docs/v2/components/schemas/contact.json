{
  "x-id": "#contact",
  "x-nter-relation": "contacts",
  "x-nter-model": "Contact",
  "x-nter-callable": true,
  "type": "object",
  "description": "Contact Information",
  "required": [
    "contact_id",
    "label",
    "email",
    "name"
  ],
  "properties": {
    "label": {
      "$ref": "./entityCommon.json#/properties/label"
    },
    "contact_id": {
      "type": "string",
      "description": "Identifier for the contact",
      "readOnly": true,
      "example": "1c022258-afcd-4bf2-a576-ae59b7a15d73",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Email address",
      "example": "chuck.reeves@zones.com",
      "nullable": true
    },
    "name": {
      "type": "string",
      "description": "Contact name",
      "example": "Chuck Reeves",
      "nullable": true
    },
    "phone": {
      "type": "string",
      "description": "Phone number",
      "example": "+1 518 867 5309",
      "nullable": true
    },
    "created": {
      "$ref": "./entityCommon.json#/properties/created"
    },
    "updated": {
      "$ref": "./entityCommon.json#/properties/updated"
    }
  }
}
