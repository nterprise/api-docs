{
  "type": "object",
  "title": "Search Link",
  "properties": {
    "_links": {
      "type": "object",
      "readOnly": true,
      "properties": {
        "nter:search": {
          "type": "object",
          "description": "Link to the /search endpoint",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "readOnly": true,
              "example": "https://api.nterprise.com/search"
            },
            "title": {
              "type": "string",
              "description": "Describes the name used for the link",
              "example": "Search"
            },
            "templated": {
              "type": "boolean",
              "readOnly": true,
              "description": "Describes the link as being a template",
              "example": true
            }
          }
        }
      }
    }
  }
}
