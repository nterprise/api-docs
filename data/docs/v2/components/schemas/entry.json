{
  "description": "Main entry point",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "properties": {
        "oauth:authorize": {
          "type": "object",
          "description": "Link for oAuth2 authorization flow",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "readOnly": true,
              "example": "https://auth.nterprise.com/oauth/authorize"
            }
          }
        },
        "oauth:logout": {
          "type": "object",
          "description": "Link for oAuth2 logout",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "readOnly": true,
              "example": "https://auth.nterprise.com/oauth/token"
            },
            "title": {
              "type": "string",
              "example": "Logout"
            },
            "method": {
              "type": "string",
              "readOnly": true,
              "example": "DELETE"
            }
          }
        },
        "oauth:token": {
          "type": "object",
          "description": "Link for Oauth2 token exchange",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "readOnly": true,
              "example": "https://auth.nterprise.com/oauth/token"
            }
          }
        },
        "nter:me": {
          "type": "object",
          "description": "Link to the /me endpoint",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "readOnly": true,
              "example": "https://api.nterprise.com/me"
            }
          }
        },
        "nter:dashboard": {
          "type": "object",
          "description": "Link to the /dashboard endpoint",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "readOnly": true,
              "example": "https://api.nterprise.com/dashboard"
            },
            "title": {
              "type": "string",
              "description": "Describes the name used for the link",
              "example": "Dashboard"
            },
            "templated": {
              "type": "boolean",
              "readOnly": true,
              "description": "Describes the link as being a template",
              "example": true
            }
          }
        },
        "nter:search": {
          "type": "object",
          "description": "Link to the /search endpoint",
          "readOnly": true,
          "properties": {
            "href": {
              "type": "string",
              "format": "url",
              "readOnly": true,
              "example": "https://api.nterprise.com/search"
            },
            "title": {
              "type": "string",
              "description": "Describes the name used for the link",
              "example": "Search"
            },
            "templated": {
              "type": "boolean",
              "readOnly": true,
              "description": "Describes the link as being a template",
              "example": true
            }
          }
        }
      }
    }
  }
}
