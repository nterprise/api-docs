{
  "description": "Defines the properties for a part unit",
  "properties": {
    "part_definition_id": {
      "type": "string",
      "format": "uuid",
      "description": "The identifier for the definition",
      "example": "ec5db56d-32e6-4e5d-9bef-1016f3e13670",
      "readOnly": true,
      "pattern": "[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}"
    },
    "customer": {
      "type": "object",
      "description": "The customer this definition is for",
      "allOf": [
        {
          "$ref": "./customer.json"
        }
      ]
    },
    "part_number": {
      "type": "string",
      "description": "Manufacture part identifier",
      "example": "602-3075-01"
    },
    "label": {
      "type": "string",
      "description": "Label for the part assigned from the manufacturer",
      "example": "iPod touch (32GB) - Blue"
    },
    "manufacturer": {
      "type": "object",
      "description": "Manufacturer information for the part",
      "allOf": [
        {
          "$ref": "./manufacturer.json"
        }
      ]
    },
    "input_filter": {
      "type": "array",
      "items": {
        "type": "object",
        "allOf": [
          {
            "$ref": "./inputFilterSpecification.json"
          }
        ]
      }
    }
  }
}
