{
  "description": "A paged unit response",
  "content": {
    "application/hal+json": {
      "schema": {
        "properties": {
          "_embedded": {
            "type": "object",
            "properties": {
              "nter:units": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "_links": {
                      "properties": {
                        "nter:current-location": {
                          "type": "object",
                          "properties": {
                            "href": {
                              "type": "string",
                              "example": "https://api.nterprise.com/units/ec5db56d-32e6-4e5d-9bef-1016f3e13670/locations/current"
                            }
                          }
                        },
                        "nter:past-locations": {
                          "type": "object",
                          "properties": {
                            "href": {
                              "type": "string",
                              "example": "https://api.nterprise.com/units/ec5db56d-32e6-4e5d-9bef-1016f3e13670/locations/past"
                            }
                          }
                        }
                      }
                    }
                  },
                  "allOf": [
                    {
                      "$ref": "../schemas/unit.json"
                    },
                    {
                      "$ref": "../schemas/links/self.json"
                    }
                  ]
                }
              }
            }
          }
        },
        "allOf": [
          {
            "$ref": "../schemas/links/nextOffset.json"
          },
          {
            "$ref": "../schemas/links/self.json"
          }
        ]
      }
    }
  }
}
