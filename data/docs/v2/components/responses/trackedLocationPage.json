{
  "description": "A paged tracked location response",
  "content": {
    "application/hal+json": {
      "schema": {
        "properties": {
          "_embedded": {
            "type": "object",
            "properties": {
              "nter:locations": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "location": {
                      "$ref": "../schemas/location.json"
                    },
                    "arrived_at": {
                      "$ref": "../schemas/trackedLocation.json#/properties/arrived_at"
                    },
                    "left_at": {
                      "$ref": "../schemas/trackedLocation.json#/properties/left_at"
                    }
                  },
                  "allOf": [
                    {
                      "$ref": "../schemas/linkSelf.json"
                    }
                  ]
                }
              }
            }
          }
        },
        "allOf": [
          {
            "$ref": "../schemas/halPaginationOffset.json"
          },
          {
            "$ref": "../schemas/linkSelf.json"
          }
        ]
      }
    }
  }
}
