{
  "description": "Part response",
  "content": {
    "application/hal+json": {
      "schema": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL to post the file to",
            "example": "https://zones.nterprise.com/upload"
          },
          "expires_in": {
            "type": "number",
            "description": "The number of seconds the upload URL is valid for",
            "example": 900
          }
        }
      }
    }
  }
}
