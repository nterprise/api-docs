{
  "description": "The part to create",
  "content": {
    "application/json": {
      "schema": {
        "type": "object",
        "required": [
          "part_number",
          "customer_id",
          "label",
          "manufacturer"
        ],
        "properties": {
          "label": {
            "$ref": "../../components/schemas/part.json#/properties/label"
          },
          "customer_id": {
            "readOnly": false,
            "description": "Id for the parts customer",
            "$ref": "../../components/schemas/customer.json#/properties/customer_id"
          },
          "manufacturer": {
            "type": "object",
            "required": [
              "manufacturer_id",
              "part_number"
            ],
            "properties": {
              "manufacturer_id": {
                "readOnly": false,
                "description": "Id for the parts manufacturer",
                "$ref": "../../components/schemas/manufacturer.json#/properties/manufacturer_id"
              },
              "part_number": {
                "readOnly": false,
                "$ref": "../../components/schemas/part.json#/properties/manufacturer/properties/part_number"
              }
            }
          },
          "input_filter": {
            "$ref": "../../components/schemas/part.json#/properties/input_filter"
          }
        }
      }
    }
  }
}
